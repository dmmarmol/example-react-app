{
    "name": "kickstart-react-redux-app",
    "author": {
        "name": "Diego Martin Marmol",
        "email": "diegomartinmarmol@gmail.com",
        "url": "http://www.diegomarmol.com"
    },
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "cross-env CI=true react-scripts build",
        "test": "react-scripts test",
        "test:watch": "yarn test --watch",
        "test:coverage": "yarn test --coverage --colors --no-watch",
        "eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
        "lint": "yarn eslint",
        "eject": "react-scripts eject"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/"
        ],
        "coverageReporters": [
            "html"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn build && lint-staged",
            "pre-push": "yarn build && lint-staged && yarn run test:coverage"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": "yarn lint",
        "src/**/*.{js,jsx}": "yarn lint"
    },
    "engines": {
        "npm": ">=6.0.0",
        "node": ">=8.0.0"
    },
    "dependencies": {
        "@material-ui/core": "^3.9.3",
        "axios": "^0.18.0",
        "color": "^3.1.0",
        "firebase": "^5.9.1",
        "kebab-case": "^1.0.0",
        "moment": "^2.24.0",
        "ramda": "^0.26.1",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-redux": "^6.0.1",
        "react-scripts": "2.1.8",
        "redux": "^4.0.1",
        "redux-form": "^8.1.0",
        "redux-saga": "^1.0.2",
        "redux-saga-firebase": "^0.15.0",
        "reselect": "^4.0.0",
        "styled-components": "^4.1.3",
        "styled-css-grid": "^1.2.1",
        "styled-icons": "^7.4.2",
        "typescript-fsa": "^3.0.0-beta-2",
        "typescript-fsa-reducers": "^1.2.0"
    },
    "devDependencies": {
        "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
        "@babel/plugin-transform-modules-commonjs": "^7.4.0",
        "@types/axios": "^0.14.0",
        "@types/color": "^3.0.0",
        "@types/jest": "^24.0.11",
        "@types/node": "^11.11.3",
        "@types/ramda": "^0.26.5",
        "@types/react": "^16.8.8",
        "@types/react-dom": "^16.8.2",
        "@types/react-redux": "^7.0.3",
        "@types/redux": "^3.6.0",
        "@types/redux-form": "^8.1.1",
        "@types/redux-saga": "^0.10.5",
        "@types/styled-components": "^4.1.12",
        "@typescript-eslint/eslint-plugin": "^1.5.0",
        "@typescript-eslint/parser": "^1.5.0",
        "cross-env": "^5.2.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-google": "^0.12.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jest": "^22.3.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "firebase-tools": "^6.5.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "node-sass": "^4.11.0",
        "prettier": "^1.16.4",
        "typescript": "^3.3.3333",
        "typescript-styled-plugin": "^0.14.0"
    }
}
