{
  "presets": ["es2015", "react-app"],
  "plugins": [
    [
      "transform-runtime",
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread",
      "react-hot-loader/babel",
      "babel-plugin-styled-components",
      {
        "pure": true,
        "displayName": false
      }
    ]
  ]
}
